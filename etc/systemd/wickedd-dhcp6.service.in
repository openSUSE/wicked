[Unit]
Description=wicked DHCPv6 supplicant service
After=local-fs.target dbus.service SuSEfirewall2_init.service
Before=wickedd.service wicked.service network.target
Conflicts=systemd-networkd.service
PartOf=wickedd.service

[Service]
Type=dbus
BusName=org.opensuse.Network.DHCP6
LimitCORE=infinity
EnvironmentFile=-/etc/sysconfig/network/config
ExecStart=@wicked_supplicantdir@/wickedd-dhcp6 --systemd --foreground
StandardError=null
Restart=on-abort

[Install]
WantedBy=wickedd.service
Alias=dbus-org.opensuse.Network.DHCP6.service

